<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mavenproject1.ClientesController">
   <children>
      <TableView fx:id="tvClientes" layoutX="1.0" layoutY="85.0" prefHeight="198.0" prefWidth="601.0">
        <columns>
          <TableColumn fx:id="colCedula" prefWidth="120.0" text="Cedula" />
          <TableColumn fx:id="colNombre" minWidth="0.0" prefWidth="120.0" text="Nombre" />
            <TableColumn fx:id="colPhone" prefWidth="120.0" text="Telefono" />
            <TableColumn fx:id="colEmail" prefWidth="120.0" text="Email" />
            <TableColumn fx:id="colNombreRepr" prefWidth="120.0" text="Representante" />
        </columns>
      </TableView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="223.0" layoutY="14.0" prefHeight="52.0" prefWidth="156.0" text="Lista de Clientes">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Button fx:id="agregarClienteButton" alignment="CENTER" layoutX="151.0" layoutY="312.0" mnemonicParsing="false" onAction="#nuevoCliente" text="Agregar Cliente">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="265.0" layoutY="312.0" mnemonicParsing="false" onAction="#editarCliente" text="Editar Cliente">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="regresarButton" layoutX="370.0" layoutY="310.0" mnemonicParsing="false" onAction="#cancelar" text="Regresar" />
   </children>
   <opaqueInsets>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </opaqueInsets>
</AnchorPane>
