<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mavenproject1.EmpleadosController">
   <children>
      <TableView fx:id="tvEmpleados" layoutY="85.0" prefHeight="198.0" prefWidth="601.0">
        <columns>
          <TableColumn fx:id="colCedula" prefWidth="120.0" text="Cedula" />
          <TableColumn fx:id="colNombre" minWidth="0.0" prefWidth="120.0" text="Nombre" />
            <TableColumn fx:id="colTelf" prefWidth="120.0" text="Telefono" />
            <TableColumn fx:id="colCorreo" prefWidth="120.0" text="Email" />
            <TableColumn fx:id="colEstado" prefWidth="120.0" text="Estado" />
        </columns>
      </TableView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="223.0" layoutY="14.0" prefHeight="52.0" prefWidth="156.0" text="Lista de Empleados">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button alignment="CENTER" layoutX="133.0" layoutY="312.0" mnemonicParsing="false" onAction="#nuevoEmpleado" text="Agregar Empleado">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="editarClienteButton" alignment="CENTER" layoutX="258.0" layoutY="312.0" mnemonicParsing="false" onAction="#editarEmpleado" text="Editar Empleado">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="368.0" layoutY="312.0" mnemonicParsing="false" onAction="#eliminarEm" text="Eliminar Empleado">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button layoutX="276.0" layoutY="349.0" mnemonicParsing="false" onAction="#cancelar" text="Menu" />
   </children>
   <opaqueInsets>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </opaqueInsets>
</AnchorPane>
